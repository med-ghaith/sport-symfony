<!DOCTYPE html>

<title>Exercice</title>

{% block body %}
    <h1>Exercice</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ exercice.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ exercice.name }}</td>
            </tr>
            <tr>
                <th>ImageUrl</th>
                <td>{{ exercice.imageUrl }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ exercice.description }}</td>
            </tr>
            <tr>
                <th>DifficultyLevel</th>
                <td>{{ exercice.difficultyLevel }}</td>
            </tr>
            <tr>
                <th>NumberOfSets</th>
                <td>{{ exercice.numberOfSets }}</td>
            </tr>
            <tr>
                <th>NumberOfRepetition</th>
                <td>{{ exercice.numberOfRepetition }}</td>
            </tr>
            <tr>
                <th>RestTime</th>
                <td>{{ exercice.restTime }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('exercice_index') }}">back to list</a>

    <a href="{{ path('exercice_edit', {'id': exercice.id}) }}">edit</a>

    {{ include('exercice/_delete_form.html.twig') }}
{% endblock %}
